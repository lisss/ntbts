/**
 * 8-way Or: 
 * out = (in[0] or in[1] or ... or in[3])
 */

CHIP Or4Way {
    IN in[4];
    OUT out;

    PARTS:
    Or (a=in[0], b=in[1], out=t01);
    Or (a=t01, b=in[2], out=t12);
    Or (a=t12, b=in[3], out=out);
}
